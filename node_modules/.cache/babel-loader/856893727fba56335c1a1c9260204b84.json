f the array\n      var days = this.lang.days.initials.slice(this.startOfWeek);\n\n      if (days.length === 7) {\n        return days;\n      } // Add the remaining days from the start of the array\n\n\n      return days.concat(this.lang.days.initials.slice(0, this.startOfWeek));\n    },\n    currentWeekStartDates: function currentWeekStartDates() {\n      return this.getWeekStartDates(this.dateInView);\n    }\n  },\n  methods: {\n    getWeekStartDates: function getWeekStartDates(dateInWeek) {\n      var date = dateUtils.clone(dateInWeek);\n      date.setDate(1); // Jump to the start of the month\n\n      date = dateUtils.moveToDayOfWeek(date, this.startOfWeek); // Jump to the start of the week\n\n      var current = dateUtils.clone(date);\n      current.setDate(current.getDate() + 7);\n      var starts = [date];\n      var month = current.getMonth();\n\n      while (current.getMonth() === month) {\n        starts.push(dateUtils.clone(current));\n        current.setDate(current.getDate() + 7);\n      }\n\n      return starts;\n    },\n    goToDate: function goToDate(date) {\n      this.$emit(\"change\", dateUtils.clone(date));\n    },\n    onDateSelect: function onDateSelect(date) {\n      this.$emit(\"date-select\", date);\n    }\n  }\n};",{"version":3,"sources":["NCalendarMonth.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgCA,OAAA,aAAA;AAEA,OAAA,SAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,gBADA;AAGA,EAAA,UAAA,EAAA;AACA,IAAA,aAAA,EAAA;AADA,GAHA;AAOA,EAAA,KAAA,EAAA;AACA,IAAA,IAAA,EAAA,MADA;AAEA,IAAA,UAAA,EAAA,QAFA;AAGA,IAAA,UAAA,EAAA,IAHA;AAIA,IAAA,QAAA,EAAA,IAJA;AAKA,IAAA,OAAA,EAAA,IALA;AAMA,IAAA,OAAA,EAAA,IANA;AAOA,IAAA,WAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA;AAFA,KAPA;AAWA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA,SAFA,CAEA;;AAFA,KAXA;AAeA,IAAA,WAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,OAAA,EAAA;AAFA;AAfA,GAPA;AA4BA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA;AACA,UAAA,IAAA,GAAA,KAAA,IAAA,CAAA,IAAA,CAAA,QAAA,CAAA,KAAA,CAA